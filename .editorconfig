root = true                         # 表示当前是项目根目录

[*]                                 # 所有文件都使用配置
charset = utf-8                     # 编码格式
indent_style = space                # Tab建缩进的样式 有space和table两种 一般代码中是space
indent_size = 4                     # 缩进为4
end_of_line = lf                    # 以lf换行 默认win为crlf mac和linux为lf
insert_final_newline = false        # 当文件保存时会自动在末尾增加一行
trim_trailing_whitespace = true     # 去掉尾行多余空格
